{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "better-npm-run test",
    "test:cov": "better-npm-run test:cov",
    "start": "node devServer",
    "build:start": "better-npm-run build:start",
    "build": "rimraf dist && better-npm-run build",
    "lint": "eslint ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack.config.prod.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node test/index.js | tap-spec",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:cov": {
      "command": "babel-node ./node_modules/.bin/isparta cover test/index.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "build:start": {
      "command": "node prodServer",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "basscss": "^7.0.4",
    "fastclick": "^1.0.6",
    "history": "1.13.*",
    "material-ui": "^0.14.0-rc2",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.2",
    "autoprefixer": "^6.1.2",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "better-npm-run": "0.0.5",
    "commitizen": "^2.4.6",
    "conventional-changelog": "^0.5.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-vesparny": "^1.2.1",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "glob": "^6.0.1",
    "html-webpack-plugin": "^1.7.0",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.0",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "postcss-loader": "^0.8.0",
    "postcss-modules-values": "^1.1.1",
    "rb-conventional-changelog": "^1.1.9",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "sinon": "^1.17.2",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "react-tap-event-plugin": "^0.2.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/rb-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-preset-stage-0": "^6.3.13",
  }
}
